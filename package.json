  "devDependencies": {
    "concurrently": "^6.4.0",
    "jest": "26.6.0",
    "mongodb-memory-server": "^8.1.0",
    "nodemon": "^2.0.15",
    "supertest": "^6.2.1"
  },
  "dependencies": {
    "@mui/x-data-grid": "^5.2.1",
    "aws-sdk": "^2.1054.0",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
